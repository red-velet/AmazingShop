<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="icu.chiou.mapper.OrderItemMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="icu.chiou.entity.OrderItemEntity">
        <result column="created" property="created"/>
        <result column="updated" property="updated"/>
        <result column="item_id" property="itemId"/>
        <result column="order_id" property="orderId"/>
        <result column="num" property="num"/>
        <result column="title" property="title"/>
        <result column="price" property="price"/>
        <result column="total_fee" property="totalFee"/>
        <result column="pic_path" property="picPath"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        created
        ,
        updated,
        item_id, order_id, num, title, price, total_fee, pic_path
    </sql>

</mapper>
